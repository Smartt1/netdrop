	<script type="5d03533c41b33b07c1b43f3a-text/javascript">
		let toggle_menu = document.querySelector('.toggle-menu')
		let menu = document.querySelector('.site-nav')
		let nav_slide = document.querySelector('.site-nav .menu-content')

		if (document.querySelector('body').clientWidth < 960) {
			toggle_menu.addEventListener('click', () => {
				menu.style.display = "flex"
				menu.style.opacity = "1"
				nav_slide.style.left = "0px"
			})
			menu.addEventListener('click', (e) => {
				if (e.target != nav_slide) {
					nav_slide.style.left = "-240px"
					menu.style.opacity = "0"
					setTimeout(() => {
						menu.style.display = "none"
					}, 500)
				}
			})
		}
	</script>

	<script type="5d03533c41b33b07c1b43f3a-text/javascript">
		window.onscroll = function() {
			if (window.pageYOffset > 90) {
				document.body.classList.add('floating-body');
			} else {
				document.body.classList.remove("floating-body");
			}
		}
	</script>

	<script type="5d03533c41b33b07c1b43f3a-text/javascript">
		let windowHeight
		window.onLoad = loftyHeight

		function loftyHeight() {
			windowHeight = window.innerHeight
		}
		loftyHeight()
	</script>

